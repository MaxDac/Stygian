{
    "workspaceFolder": "/workspace",
    "name": "Phoenix + Postgres",
    "dockerComposeFile": "docker-compose.yml",
    "service": "web",
    "postCreateCommand": "mix deps.get && mix deps.compile && mix ecto.create && mix ecto.migrate && mix phx.server",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "jakebecker.elixir-ls"
            ]
        }
    }
}